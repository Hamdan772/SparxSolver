{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "GOOGLE_AI_API_KEY": "@google_ai_api_key"
  },
  "build": {
    "env": {
      "GOOGLE_AI_API_KEY": "@google_ai_api_key"
    }
  },
  "functions": {
    "app/api/**/*": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}